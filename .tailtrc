---
configs:
  log:
    post_process:
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:TRACE)
            enabled: 1
            colour:
              - bold
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:DEBUG|SLF4J)
            enabled: 1
            colour:
              - blue
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:INFO)
            enabled: 1
            colour:
              - green
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:Warn|WARN(?:ING)?)
            enabled: 1
            colour:
              - yellow
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:Error|ERROR|SEVERE)
            enabled: 1
            colour:
              - red
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:FATAL)
            enabled: 1
            colour:
              - magenta
              - bold
          - !!perl/hash:Tail::Tool::Regex
            regex: !!perl/regexp (?-xism:\d\d/\d\d/\d\d\d\d|\d\d:\d\d:\d\d|\d\d\d\d/\d\d/\d\d)
            enabled: 1
            colour:
              - red
              - bold
  log4j:
    post_process:
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - bold
            enabled: 1
            regex: !!perl/regexp (?-xism:TRACE)
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - blue
            enabled: 1
            regex: !!perl/regexp (?-xism:DEBUG|SLF4J)
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - green
            enabled: 1
            regex: !!perl/regexp (?-xism:INFO)
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - yellow
            enabled: 1
            regex: !!perl/regexp (?-xism:WARN(?:ING)?)
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - red
            enabled: 1
            regex: !!perl/regexp (?-xism:ERROR|SEVERE)
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - magenta
              - bold
            enabled: 1
            regex: !!perl/regexp (?-xism:FATAL)
  log4perl:
    post_process:
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - blue
            enabled: 1
            regex: !!perl/regexp (?-xism:(?<=\s)DEBUG(?=\s))
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - green
            enabled: 1
            regex: !!perl/regexp (?-xism:(?<=\s)INFO(?=\s))
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - yellow
            enabled: 1
            regex: !!perl/regexp (?-xism:(?<=\s)WARN(?=\s))
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - red
            enabled: 1
            regex: !!perl/regexp (?-xism:(?<=\s)ERROR(?=\s))
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - magenta
              - bold
            enabled: 1
            regex: !!perl/regexp (?-xism:(?<=\s)FATAL(?=\s))
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - magenta
            enabled: 1
            regex: '[{]"\w\w\w:\w+Request":([{](?:(?:(?>[^{}]+|(?1))+))[}])[^}]*[}]'
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - cyan
            enabled: 1
            regex: '[{]"\w\w\w:\w+Response":([{](?:(?:(?>[^{}]+|(?1))+))[}])[^}]*[}]'
#          - !!perl/hash:Tail::Tool::Regex
#            colour:
#              - on_green
#            enabled: 1
#            regex: '[{](?:[^{}]+|(?1))+[}]'
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - bold
            enabled: 1
            regex: '\[\d\d\d\d/\d\d/\d\d\s+\d\d:\d\d:\d\d\]'
    pre_process:
      - !!perl/hash:Tail::Tool::Plugin::GroupLines
        regex:
          - !!perl/hash:Tail::Tool::Regex
            enabled: 1
            regex: !!perl/regexp '(?-xism:^\[\d\d\d\d/\d\d/\d\d \d\d:\d\d:\d\d\])'
      - !!perl/hash:Tail::Tool::Plugin::Spacing
        last_time: 1300055107
        lines:
          - 2
          - 5
        many: 0
        post: 0
        times:
          - 2
          - 5
      - !!perl/hash:Tail::Tool::Plugin::Ignore
        many: 1
        post: 0
        regex:
          - !!perl/hash:Tail::Tool::Regex
            enabled: 0
            regex: !!perl/regexp (?-xism:NScreens::ZooScript|NScreens::View::TT)
      - !!perl/hash:Tail::Tool::Plugin::Ignore
        many: 1
        post: 0
        regex:
          - !!perl/hash:Tail::Tool::Regex
            enabled: 0
            regex: !!perl/regexp (?-xism:NScreens::UserSubscriptions|NScreens::Auth|NScreens::Systems::OIETS|NScreens::Session)
  mojo:
    post_process:
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        colourer: &1 !!perl/code '{ "DUMMY" }'
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - red
            enabled: 1
            regex: !!perl/regexp '(?-xism:\[error\])'
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        colourer: *1
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - yellow
            enabled: 1
            regex: !!perl/regexp '(?-xism:\[warn\])'
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        colourer: *1
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - blue
            enabled: 1
            regex: !!perl/regexp '(?-xism:\[(?:info|notice)\])'
      - !!perl/hash:Tail::Tool::Plugin::Highlight
        colourer: *1
        many: 1
        post: 1
        regex:
          - !!perl/hash:Tail::Tool::Regex
            colour:
              - green
            enabled: 1
            regex: !!perl/regexp '(?-xism:\[debug\])'
    pre_process: []
