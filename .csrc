
<file_types>
    <csv>
        definite = [.](?:csv|tab)$
    </csv>
    <tt>
        definite = [.]tt2?$
        #possible
        #none
        #bang
        #other_types
    </tt>
    <data>
        definite = [.]dat$
    </data>
    <jar>
        definite = [.](?:jar|jsa|dll)$
    </jar>
    <zoo>
        definite = [.]zoo$
    </zoo>
    <pm>
        definite = [.]pm$
    </pm>
    <ignore>
        definite = cmds
        definite = tags
        definite = errors.err
        definite = [.].*[.]sw[nmopqrx]$
        definite = vrc[.]yml
        definite = [.]prove
        definite = [.]trash
        definite = [.](?:war|jar|zip|tar)$
        definite = [.]meteor/local/
        definite = [.]gradle/
        definite = platforms/
        definite = [.](?:js|css)[.]map$
        definite = [.]sass-cache/
        definite = node_modules/
    </ignore>
    <1pfe>
        other_types = ftl
        other_types = javascript
    </1pfe>
</file_types>
